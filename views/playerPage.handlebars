<html>
<head>
</head>

<link rel="stylesheet" href="/specificGamePage.css">
<div class="Game-Wrapper">
<div  class="TitleBox">
  <div class="TitleBox-title">Search Results</div>
  {{!-- <input class="typeahead tt-query" spellcheck="false" autocomplete="off" name="typeahead" type="text" /> --}}
  <form action="/Players/search" method="post">
    <input name="typeahead" type="text" >
    <input type="submit" value="Search Player" >
  </form>


</div>
<div class="Table-Wrapper">
<table class="allPlayers">
     <caption class="Table-Title">All Players</caption>



{{#each players}}
  <tr>
    <td class="playerTable" onclick="window.location.href='/user/{{ userName }}'">
        <img src={{ profilePictures }} class="playerImage" width="100" height="100">
        <div class="playerName">{{ userName }}</div>
    </td>

  </tr>
{{/each}}



</table>
</div>

</div>
</html>
