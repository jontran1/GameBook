<html>
<head>
</head>

<body>
<link rel="stylesheet" href="/gamePage.css">
<div class="Content-Wrapper">

<div class="AllGames-Wrapper">
{{#each games}}
        <div class="GameBox">
  <button class="btn" onclick="window.location.href='/Games/{{gameID}}'"> <img src={{ gameImage }}></img> </button>
</div>
    {{/each}}
</div>


    <div class="MyTable">
    <table>
      <caption class="Table-Title">Top Games</caption>
        <tr>
          <th>Game</th>
          <th>Hours</th>
          
        </tr>
        {{#each gameList}}
        <tr>

          <td>{{ gameName }}</td>
          <td>{{ tHours }}</td>
        
        </tr>
        {{/each}}
    </table>
    </div>




</body>
</html>







