<html>
<head>
</head>

<body>
<link rel="stylesheet" href="/gamePage.css">

<div class="AllGames-Wrapper">
{{#each games}}
        <div class="GameBox">
  <button class="btn" onclick="window.location.href='/Games/{{gameID}}'"><img src={{ gameImage }}></button>
</div>
    {{/each}}
</div>


<table>
  <tr>
    <th>Top Games</th>
  </tr>

{{#each topGames}}
  <tr>
    <td onclick="window.location.href='{{ page }}'">{{ name }}</td>
  </tr>
{{/each}}

</table>
</body>

</html>







